<DemoViewer
    @cameraPosition={{hash x=0 y=5 z=10}}
    @cameraLookAt={{hash x=0 y=0 z=0}}
    @onSceneInsert={{this.onSceneInsert}}
    @controlDef={{this.controlDef}}
    @onControlChanged={{this.onControlChanged}}
as |parent|>
    <SceneObject
        @parent={{parent}}
        @type='GridHelper'
        @constructorProps={{array 100 20 (to-number "0x3486eb") (to-number "0x666666")}}
    />
</DemoViewer>